     ]
    }
   ],
   "source": [
    "#下面是一个例子\n",
    "import torch\n",
    "a = torch.randint(low=0, high=10, size=(1,10))\n",
    "print(a)\n",
    "a = torch.clamp(a, 3,9)\n",
    "print(a)\n"
   ]
